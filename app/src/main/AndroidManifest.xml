<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dolaapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_pic"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DolaApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".ConversationScreen"/>
        <activity android:name=".AppInfo" />
        <activity android:name=".Others.SignUp.SignUp_3" />
        <activity android:name=".Others.SignUp.SignUp_2" />
        <activity android:name=".Others.SignUp.SignUp_1" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".LoginScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>